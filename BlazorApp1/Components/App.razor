<body>
    <Routes />
    <script src="_framework/blazor.web.js"></script>
    <script>
        function TestDataTablesAdd(table) {
            $(document).ready(function () {
                $(table).DataTable();
            });
        }
    </script>

    <script>
        function TestDataTablesRemove(table) {
            $(document).ready(function () {
                $(table).DataTable().destroy();
            });
        }
    </script>
</body>

</html>
